{
    "apiVersion": "v1",
    "kind": "ReplicationController",
    "metadata": {
        "name": "cubesdb-controller",
        "labels": {
            "name": "cubesdb"
        }
    },
    "spec": {
        "replicas": 1,
        "template": {
            "metadata": {
                "labels": {
                    "name": "cubesdb"
                }
            },
            "spec": {
                "containers": [
                    {
                        "image": "gcr.io/polished-vault-462/cubesdb:v1",
                        "name": "cubesdb",
                        "env": [],
                        "ports": [
                            {
                                "containerPort": 5000,
                                "name": ""
                            }
                        ]
                    }
                ]
            }
        }
    }
}